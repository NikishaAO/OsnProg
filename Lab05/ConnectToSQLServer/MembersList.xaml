        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConnectToSQLServer"
        mc:Ignorable="d"
        Title="MembersList" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="MembersList1" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,45,246"/>
        <ComboBox x:Name="CB" HorizontalAlignment="Left" Margin="400,235,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="CB_SelectionChanged"/>
        <DataGrid x:Name="MembersComissions" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,235,443,51"/>
        <Button x:Name="HomeButton" Content="Назад" HorizontalAlignment="Left" Margin="674,340,0,0" VerticalAlignment="Top" Height="43" Width="81" Click="HomeButton_Click"/>

    </Grid>
</Window>
